{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 50,
      "minLength": 3
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "imagesMock": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text",
      "default": "100% Algod√£o"
    },
    "seller": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::seller.seller",
      "inversedBy": "products"
    },
    "sub_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-category.sub-category",
      "inversedBy": "products"
    },
    "imageDesign": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "basePrice": {
      "type": "decimal",
      "required": true,
      "default": 55.99,
      "min": 1.99
    }
  }
}
